{
  "containerDefinitions": [
        {
            "name": "app",
            "image": "363254031734.dkr.ecr.ap-northeast-1.amazonaws.com/node-application:latest",
            "portMappings": [
                { 
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "family": "cluster-family",
    "executionRoleArn": "arn:aws:iam::363254031734:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}
